.container.user-page
  %header.user-nav
    .media
      .media-left
        %a{href: "#"}
          %img.media-object{alt: "64x64", "data-holder-rendered": "true", "data-src": "holder.js/64x64", src: @user.avatar, style: "width: 64px; height: 64px;"}/
      .media-body
        %h4#top-aligned-media.media-heading
          = @user.username
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
            = @user.member
        .row
          .col-md-6
            %h4 Profile
            %p
              = @user.profile
          .col-md-6
            %h4 Works
            %p
              = @user.works
.container.proto-list
  .row
    - @prototypes.each do |prototype|
      = render partial:"prototypes/prototype", locals:{prototype: prototype}

.text-center
  %ul.pagination
    %li.disabled
      %a{"aria-label" => "Previous", href: "#"}
        %span{"aria-hidden" => "true"} Â«
    %li.active
      %a{href: "#"}
        1
        %span.sr-only (current)
    %li
      = link_to "2", "#"
    %li
      = link_to "3", "#"
    %li
      = link_to "4", "#"
    %li
      = link_to "5", "#"
