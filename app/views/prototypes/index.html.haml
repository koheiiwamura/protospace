= render partial:"top"
.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        %li.active{role: "presentation"}
          = link_to "Popular PROTO", prototypes_popular_index_path
        %li{role: "presentation"}
          = link_to "Newest Proto", prototypes_newest_index_path
      = button_to "View Tags", tags_path, class: "btn btn-default col-lg-1", method:"get"

.container.proto-list
  .row
    - @prototypes.each do |prototype|
      = render partial:"prototype", locals:{prototype: prototype}

.text-center
  = paginate @prototypes
